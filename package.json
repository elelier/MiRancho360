{
  "name": "mirancho360",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "prepare": "husky install",
    "preinstall": "npx --no-install npm@latest --version >/dev/null 2>&1 || true",
    "docs:fix": "node -e \"const fs=require('fs');const p='docs';function fix(f){let s=fs.readFileSync(f,'utf8');s=s.replace(/(?m)\\s+$/g,'');fs.writeFileSync(f,s);} ;require('fs').readdirSync(p).forEach(function(f){const fp=p+'/'+f;try{if(fs.lstatSync(fp).isFile()&&fp.endsWith('.md'))fix(fp);}catch(e){}});console.log('docs:fix complete')\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "setup-db": "node scripts/setup-database.js",
    "db:setup": "node scripts/setup-database.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@types/react-router-dom": "^5.3.3",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.0",
    "vite": "^7.1.7"
  }

  ,
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx,md,mdx}": [
      "npm run docs:fix",
      "eslint --fix",
      "git add"
    ]
  }
}
